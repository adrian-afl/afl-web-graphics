    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "npm-check-updates": "^17.1.11",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "tsx": "^4.19.2",
